<!DOCTYPE html>
<html lang="en">
  <head>
    <% pageName = "Contest"; %>
    <%- include('./partials/head') %>
    <style>
      .sidebar{
        margin-top: 1em;
      }
    </style>
  </head>
  <body>
    

    <%- include('partials/topbar') %>

    <div class="container">
      <div class="row">
        
      <%- include('partials/contestsidebar') %>
    
        
        <main class="col-md-9 main-content">
          
          <%- include('partials/tray') %>
        
          
          <div id="post-results" class="my-3 p-3 bg-body rounded shadow-sm">
            <% if (posts.length > 0) { %>
              <% posts.forEach(post => { %>
                <%- include('partials/dreamForContest', { post }) %>
              <% }); %>
            <% } else { %>
              <%- include('partials/emptyPost') %>
            <% } %>
          </div>
        </main>
        
        
      </div>
    </div>
    
    
   
    <%- include('partials/footer') %>
    
  </body>
</html>
